<struts>
	<package name="filterConfig" extends="struts-default" namespace="/">
		<action name="GoLogin" class="com.myweb.action.GoLogin">
			<result name="input">/login.jsp</result>
			<result name="success">/login_ok.jsp</result>
			<!-- <result name="success">/index2.jsp</result> -->
		</action>
		<action name="RegistAc" class="com.myweb.action.RegistAction">
			<result name="input">/regist.jsp</result>
			<result name="success">/login.jsp</result>
			<!-- <result name="success">/index2.jsp</result> -->
		</action>
		<action name="ShopCommentAc" class="com.myweb.action.ShopCommentAc">
			<result name="input">/regist.jsp</result>
			<result name="success">/shopDetail.jsp</result>
			<!-- <result name="success">/index2.jsp</result> -->
		</action>
		<action name="ArticleAction" class="com.myweb.action.ArticleAction">
			<result name="input">/login.jsp</result>
			<result name="articleContent">/articleContent.jsp</result>
			<result name="backWriteArticle">/writeArticle.jsp</result>
			<result name="success">/article.jsp</result>
			<!-- <result name="success">/index2.jsp</result> -->
		</action>
		<action name="TestAction" class="com.myweb.action.TestAction">
			<result name="input">/login.jsp</result>
			<result name="articleContent">/articleContent.jsp</result>
			<result name="success">/test.jsp</result>
			<!-- <result name="success">/index2.jsp</result> -->
		</action>
			<action name="RecAction" class="com.myweb.action.RecommendAc">
			<result name="input">/login.jsp</result>
			<result name="success">/rec.jsp</result>
			<!-- <result name="success">/index2.jsp</result> -->
		</action>
		
		<action name="VisitorAction" class="com.myweb.action.VisitorAction">
			<result name="input">/article.jsp</result>
			<result name="success">/article.jsp</result>
			<!-- <result name="success">/index2.jsp</result> -->
		</action>
	</package>
